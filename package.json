{
  "name": "relationship-journal",
  "version": "1.0.0",
  "description": "A web application for couples to strengthen their relationship",
  "private": true,
  "scripts": {
    "install:all": "cd backend && npm install && cd ../frontend && npm install",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "start:backend": "cd backend && npm start",
    "build": "cd frontend && npm run build",
    "prod:setup": "npm run install:all && npm run build",
    "prod:start": "pm2 start ecosystem.config.js",
    "prod:stop": "pm2 stop relationship-journal-app relationship-journal-tunnel",
    "prod:restart": "pm2 restart relationship-journal-app relationship-journal-tunnel",
    "prod:logs": "pm2 logs",
    "prod:status": "pm2 status"
  },
  "keywords": ["relationship", "journal", "couples"],
  "author": "",
  "license": "MIT"
}
